{
	"name": "netm8",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev --config config/vite.config.ts",
		"build": "wrangler types && tsc -b && vite build --config config/vite.config.ts",
		"check": "biome check . && tsc -b --noEmit && node scripts/lint-docs.mjs && mkdir -p dist/client && vitest run --config config/vitest.config.ts",
		"lint": "biome check --write .",
		"lint:check": "biome check .",
		"test": "mkdir -p dist/client && vitest run --config config/vitest.config.ts",
		"test:watch": "mkdir -p dist/client && vitest --config config/vitest.config.ts",
		"seed": "node scripts/seed.mjs",
		"migrate": "wrangler d1 migrations apply DB --remote",
		"deploy:staging": "gh workflow run pipeline.yml",
		"deploy:production": "gh workflow run pipeline.yml -f environment=production",
		"check:docs": "node scripts/lint-docs.mjs",
		"check:bindings": "node scripts/check-bindings.mjs",
		"tail:dev": "wrangler dev -c workers/tail/wrangler.jsonc",
		"tail:deploy": "wrangler deploy -c workers/tail/wrangler.jsonc",
		"test:visual": "playwright test --config config/playwright.config.ts",
		"test:visual:update": "playwright test --config config/playwright.config.ts --update-snapshots"
	},
	"dependencies": {
		"@ai-sdk/react": "^3.0.105",
		"@cloudflare/ai-chat": "^0.1.5",
		"@cloudflare/sandbox": "^0.7.6",
		"@hono/zod-validator": "^0.7.6",
		"@radix-ui/react-use-controllable-state": "^1.2.2",
		"@rive-app/react-webgl2": "^4.27.0",
		"@streamdown/cjk": "^1.0.2",
		"@streamdown/code": "^1.0.3",
		"@streamdown/math": "^1.0.2",
		"@streamdown/mermaid": "^1.0.2",
		"@tailwindcss/vite": "^4.2.1",
		"@tanstack/react-query": "^5.90.21",
		"@tanstack/react-router": "^1.163.2",
		"@xyflow/react": "^12.10.1",
		"agents": "^0.6.0",
		"ai": "^6.0.101",
		"ansi-to-react": "^6.2.6",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"drizzle-orm": "^0.45.1",
		"embla-carousel-react": "^8.6.0",
		"hono": "^4.12.3",
		"hono-agents": "^3.0.6",
		"lucide-react": "^0.575.0",
		"media-chrome": "^4.18.0",
		"motion": "^12.34.3",
		"nanoid": "^5.1.6",
		"radix-ui": "^1.4.3",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-jsx-parser": "^2.4.1",
		"shiki": "^4.0.0",
		"streamdown": "^2.3.0",
		"tailwind-merge": "^3.5.0",
		"tailwindcss": "^4.2.1",
		"tokenlens": "^1.3.1",
		"use-stick-to-bottom": "^1.1.3",
		"workers-ai-provider": "^3.1.2",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@cloudflare/vite-plugin": "^1.25.6",
		"@cloudflare/vitest-pool-workers": "^0.12.18",
		"@commitlint/cli": "^20.4.2",
		"@commitlint/config-conventional": "^20.4.2",
		"@playwright/test": "^1.58.2",
		"@tanstack/react-query-devtools": "^5.91.3",
		"@tanstack/router-plugin": "^1.163.2",
		"@types/node": "^25.3.2",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"drizzle-kit": "^0.31.9",
		"lefthook": "^2.1.1",
		"typescript": "~5.9.3",
		"vite": "^7.3.1",
		"vitest": "^3.2.0",
		"wrangler": "^4.69.0"
	}
}
