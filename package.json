{
	"name": "netm8",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:worker": "wrangler dev",
		"build": "tsc -b && vite build",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc -b --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"preview": "npm run build && vite preview",
		"deploy:staging": "npm run build && wrangler deploy --env staging --config wrangler.jsonc",
		"deploy:production": "npm run build && wrangler deploy --env production --config wrangler.jsonc",
		"cf-typegen": "wrangler types"
	},
	"dependencies": {
		"@hono/zod-validator": "^0.7.6",
		"@tanstack/react-query": "^5.90.21",
		"@tanstack/react-router": "^1.163.2",
		"drizzle-orm": "^0.45.1",
		"hono": "^4.12.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@cloudflare/vite-plugin": "^1.25.5",
		"@cloudflare/vitest-pool-workers": "^0.8.0",
		"@commitlint/cli": "^20.4.2",
		"@commitlint/config-conventional": "^20.4.2",
		"@tanstack/react-query-devtools": "^5.91.3",
		"@tanstack/router-plugin": "^1.163.2",
		"@types/node": "^24.10.14",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"drizzle-kit": "^0.31.9",
		"lefthook": "^2.1.1",
		"typescript": "~5.9.3",
		"vite": "^7.3.1",
		"vitest": "^3.2.0",
		"wrangler": "^4.68.1"
	}
}
